<script>
function showSlogan() {
    var country;
    if(CookieUtil.getCookie("country_iso")){
        country = CookieUtil.getCookie("country_iso");
    } else {
        country = "GB";
    }
    var allBoxes = new Array();
    switch (country) {
        case "US": $(".slogan-usa-shipping").show(); break;
    }
}
</script>


<script>
document.body.onload = function() {
    showSlogan();
}
</script>

<script language="Javascript">
function transitionBox(from, to, arr) {
    function next() {
        var nextTo;
        if (to.is(":last-child")) {
            nextTo = arr[0];
        } else {
            nextTo = arr[1];
        }
        to.fadeIn(500, function () {
            setTimeout(function () {
                transitionBox(to, nextTo, arr);
            }, 5000);
        });
    }
    
    if (from) {
        from.fadeOut(500, next);
    } else {
        next();
    }
}

</script></div>
