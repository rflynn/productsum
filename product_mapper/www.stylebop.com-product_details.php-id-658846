								$('#jcarousel_bottom ul').width(liCount * 160);

								$('#jcarousel_bottom').jcarousel({wrap: 'circular'});
								var jcarousel = $('#jcarousel_bottom').data('jcarousel');

								$('#shop_the_look_images_top').click(function () {
									jcarousel.scroll('-=1');
								});

								$('#shop_the_look_images_bottom').click(function () {
									jcarousel.scroll('+=1');
								});
							})(jQuery);
						</script>
					</div>
