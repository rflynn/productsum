						<script type="text/javascript">

							//check if the currently selected size is available
							function checkSizeAvailability() {
								var onesize = 0;

								//if not onesize
								if (onesize == false) {
									//check if a size is selected
									if (document.getElementById("itemReference").selectedIndex == 0) {
										alert('Please choose a size.');
										return;
									}
								}

								for (i = 0; i < document.setSize.reference.length; ++i) {
									if (document.setSize.reference.options[i].selected == true && document.setSize.reference.options[i].value.substring(0, 12) == "featherlight") {
										$.featherlight({
											ajax: '/popup.php?name=forecast&article_id=658846&reference_id='+ document.setSize.reference.options[i].value.substring(26, document.setSize.reference.options[i].value.length)
										});
										return false;
									}
								}
								return true;
							}



						</script>
						

