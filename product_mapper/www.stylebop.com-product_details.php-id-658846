		<script type="text/javascript">
			(function() {
				$(window).resize(function() {
					$('#ui-id-1').css({ position : 'absolute', left : 'auto', left : ($('.search-container').offset().left+0), top : ($('.search-container').offset().top+$('.search-container').outerHeight()) });
				});
				var flAutocompleteReadyHandler = function(event) {
					var flAutocompleteObject = event.detail.flAutocomplete,
						fljQuery = event.detail.fljQuery,
						flAutocompleteUiWidget = event.detail.flAutocompleteUiWidget,
						autocompleteUlElement = event.detail.autocompleteUlElement;

					flAutocompleteObject.afterRendering = function(event) {
						autocompleteUlElement.css({ position : 'absolute', left : 'auto', left : ($('.search-container').offset().left+0), top : ($('.search-container').offset().top+$('.search-container').outerHeight()) });
					};
				};

				var readyEventName = 'flAutocompleteReady';

				if (document.addEventListener) {
					document.addEventListener(readyEventName, flAutocompleteReadyHandler, false);
				} else if (document.attachEvent)  {
					document.attachEvent(readyEventName, flAutocompleteReadyHandler);
				}

			})();
			(function() {
				var flAutocompleteReadyHandler = function(event) {
										var sslHost = (("https:" == document.location.protocol) ? "https://" : "http://");
					event.detail.flAutocomplete.shopConfig.autocompleteProxy = sslHost+'www.stylebop.com/scripts/findologic/structured_autocomplete_US.php';
				};

				var readyEventName = 'flAutocompleteReady';

				if (document.addEventListener) {
					document.addEventListener(readyEventName, flAutocompleteReadyHandler, false);
				} else if (document.attachEvent)  {
					document.attachEvent(readyEventName, flAutocompleteReadyHandler);
				}

			})();
		</script>


